<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="l10n_ao_account_report_form" model="ir.ui.view">
        <field name="name">account.report.form.inherit</field>
        <field name="model">account.report</field>
        <field name="inherit_id" ref="account_reports.account_report_form"/>
        <field name="arch" type="xml">
            <field name="code" position="after">
                <field name="note"/>
            </field>
            <field name="filter_account_type" position="after">
                <field name="filter_by_account"/>
            </field>
        </field>
    </record>
</odoo>