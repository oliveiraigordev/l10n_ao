<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="l10n_ao_holidays_calendar_view" model="ir.ui.view">
        <field name="name">L10n_ao_holidays - Inherited Calendar Form View</field>
        <field name="model">hr.leave</field>
        <field name="inherit_id" ref="hr_holidays.hr_leave_view_dashboard"/>
        <field name="arch" type="xml">
            <xpath expr="//calendar" position="attributes">
                <attribute name="js_class">l10n_ao_time_off_calendar_dashboard</attribute>
            </xpath>
        </field>
    </record>
</odoo>