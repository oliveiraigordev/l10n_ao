<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">

    <record id="sequence_debit_note_accounting" model="ir.sequence">
        <field name="code">report.debit.note.accounting</field>
        <field name="name">Nota de Débito (Clientes)</field>
        <field name="prefix">ND /%(range_year)s/</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
        <field name="use_date_range">True</field>
        <field name="implementation">no_gap</field>
        <field name="company_id" eval="False"/>
    </record>

    <record id="sequence_debit_note_vendor" model="ir.sequence">
        <field name="code">report.debit.note.vendor</field>
        <field name="name">Nota de Débito (Fornecedores)</field>
        <field name="prefix">ND /%(range_year)s/</field>
        <field name="padding">4</field>
        <field name="number_increment">1</field>
        <field name="use_date_range">True</field>
        <field name="implementation">no_gap</field>
        <field name="company_id" eval="False"/>
    </record>

</odoo>



