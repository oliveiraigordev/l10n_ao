<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="inherit_view_pos_tree" model="ir.ui.view">
        <field name="name">Point of sale Inherit tree</field>
        <field name="model">pos.order</field>
        <field name="inherit_id" ref="point_of_sale.view_pos_order_tree"/>
        <field name="arch" type="xml">
            <field name="name" position="before">
                <field name="pos_number"/>
            </field>
        </field>
    </record>
    <record id="inherit_view_pos_form" model="ir.ui.view">
        <field name="name">Point of sale Inherit form</field>
        <field name="model">pos.order</field>
        <field name="inherit_id" ref="point_of_sale.view_pos_pos_form"/>
        <field name="arch" type="xml">
            <field name="name" position="after">
                <field name="pos_number"/>
            </field>
            <field name="fiscal_position_id" position="after">
                <field name="hash" invisible="1"/>
                <field name="sequence_number"/>
            </field>
        </field>
    </record>
</odoo>